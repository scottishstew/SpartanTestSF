        <li id="lnkItemNo" ><a id="hlItemNo" class="tabLink" href="#">Item No</a></li>
        </ul>
        <div id="UnitNoTab">
            <div class="tabcontainer">
                @Html.TextBox("txtSearchUnitNo", null, new { @placeholder = "Enter The Unit Number Search Term" })
