            </div>
     </div>

    <div id="ItemNoTab">
        <div  class="tabcontainer">
            @Html.TextBox("txtSearchItemNo", null, new { @placeholder = "Enter The Item Number Search Term" })
            <input class="btnSearchSubmit" type="image" src="~/Images/mg.png" id="btnSearchItemNo" />

        </div>
    </div>
</div>
