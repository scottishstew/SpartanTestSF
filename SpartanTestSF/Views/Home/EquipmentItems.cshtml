@model SpartanTestSF.Models.HomeModel

<div id="dvResults">
    <span id="spnResults">Results</span>
    <a id="lnkReset" href="#">Reset</a>
</div>

<table style="width:100%;" id="tblEquipmentItems">
    @foreach (var equipmentItem in Model.EquipmentItems.SerialisedEquipment)
    {
        <tr>
            <td>
                <div class="eqItem">
                    <img src="~/Images/arrow.png" class="imgArrow" />
                    <span class="txtUnitNo">@equipmentItem.ExternalId</span> <br />
                    @equipmentItem.EquipmentType.ExternalId <br />
                    @equipmentItem.EquipmentType.Description <br /><br />
                </div>
            </td>
            
        </tr>
    }
   
  </table>
